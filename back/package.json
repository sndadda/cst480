    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.7.4",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.15",
    "@types/jest": "^29.2.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.11.18",
    "axios": "^1.2.2",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
